{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^8.0",
        "laravel/jetstream": "^1.5",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "*",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "moneyphp/money": "^3.3",
        "phpoffice/phpspreadsheet": "^1.17.1",
        "predis/predis": "~1.0",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/data-transfer-object": "^2.8",
        "squizlabs/php_codesniffer": "^3.5.8"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpro/grumphp": "^1.3",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.4",
        "psalm/plugin-phpunit": "^0.16.1",
        "vimeo/psalm": "^4.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Barrigudinha\\": "barrigudinha/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Integrations\\": "integrations/",
            "Src\\": "src/",
            "Src\\Notifications\\": "src/notifications",
            "Src\\Prices\\": "src/prices",
            "Src\\Products\\": "src/products"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Unit\\App\\": "tests/Unit/app/",
            "Tests\\Unit\\Barrigudinha\\": "tests/Unit/barrigudinha/",
            "Tests\\Unit\\Integrations\\": "tests/Unit/integrations/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "setup-dev": [
            "@post-root-package-install",
            "@composer install",
            "php artisan key:generate",
            "php artisan migrate:refresh --seed"
        ]
    }
}
